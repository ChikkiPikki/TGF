<!DOCTYPE html>
<html>
<head>
	<title>Admin Login | TecSo Foundation</title>
	<%- include("dynamicCommons.ejs") %>
</head>
<body>
	<%- include("header.ejs") %>

	<div class="page-content" style="margin-top: 0rem; padding: 4rem;">
    <h1 class="ack">Login!</h1>
		<form action="/login" method="POST">
  <div class="form-group">
    <label for="username">Username</label>
    <input  name="username" class="form-control" id="username" aria-describedby="emailHelp" placeholder="Enter username">
    
  </div>
  <div class="form-group">
    <label for="password">Password</label>
    <input type="password" name="password" class="form-control" id="password" placeholder="Password">
  </div>
  
  <button type="submit" class="btn btn-primary">Login</button>
</form>
	</div>






	<%- include("footer.ejs") %>

</body>
</html>